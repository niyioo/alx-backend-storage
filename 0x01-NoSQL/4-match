#!/bin/bash
# Script to list all documents with name="Holberton school" in the collection school

# Check if the database name is provided
if [ -z "$1" ]; then
    echo "Usage: $0 <database_name>"
    exit 1
fi

# Connect to MongoDB and list matching documents in the school collection
mongo "$1" --quiet --eval 'db.school.find({ name: "Holberton school" }).forEach(printjson);'
